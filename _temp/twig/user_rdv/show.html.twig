{% extends 'base.html.twig' %}

{% block title %}Rdv{% endblock %}

{% block body %}
    <h1>Rdv</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rdv.id }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ rdv.description }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ rdv.slug }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ rdv.date ? rdv.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Time</th>
                <td>{{ rdv.time ? rdv.time|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ rdv.createdAt ? rdv.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ rdv.updatedAt ? rdv.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_rdv_index') }}">back to list</a>

    <a href="{{ path('app_user_rdv_edit', {'id': rdv.id}) }}">edit</a>

    {{ include('user_rdv/_delete_form.html.twig') }}
{% endblock %}
