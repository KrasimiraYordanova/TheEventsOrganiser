<form style="display:inline-block" method="post" action="{{ path('app_user_expense_delete', {'id': eventList.id, 'expense_id': expense.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ expense.id) }}">
    <button class="form__button-i btn btn-effacer"><i class="fa-solid fa-trash"></i></button>
</form>
