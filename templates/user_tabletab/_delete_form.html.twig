<form style="display:inline-block" method="post" action="{{ path('app_user_tabletab_delete', {'eventSlug': eventList.eventSlug, 'tabletab_id': tabletab.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tabletab.id) }}">
    <button class="form__button-i btn btn-effacer"><i class="fa-solid fa-trash"></i></button>
</form>
