{% extends 'base.html.twig' %}

{% block title %}Picture{% endblock %}

{% block body %}
    <h1>Picture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ picture.id }}</td>
            </tr>
            <tr>
                <th>NamePath</th>
                <td>{{ picture.namePath }}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ picture.slug }}</td>
            </tr>
            <tr>
                <th>Album</th>
                <td>{{ picture.album }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ picture.createdAt ? picture.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ picture.updatedAt ? picture.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_wedding_index') }}">back to list</a>

    <a href="{{ path('app_user_wedding_edit', {'id': picture.id}) }}">edit</a>

    {{ include('user_wedding/_delete_form.html.twig') }}
{% endblock %}
